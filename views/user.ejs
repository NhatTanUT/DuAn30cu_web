		<div class="row">
			<div class="col-12 col-m-12 col-sm-12">
				<div class="card">
					<div class="card-header">
						<h3>
							Danh sach lop hoc
						</h3>
						<!-- <i class="fas fa-ellipsis-h"></i> -->
						<input type="date" name="" id="choose-date">
						<button class="btn-choose-date">Get</button>
					</div>
					<div class="card-content">
						<!-- <input type="datetime-local" name="date" class="input-date"> -->
						<table>
							<thead>
								<tr>
									<th>STT</th>
									<th>ID lớp</th>
									<th>Tên lớp</th>
									<th>ID GV</th>
									<th>Tên GV</th>
									<th>Date</th>
									<th>Danh sách học viên</th>
								</tr>
							</thead>
							<tbody>
								<% if (listTimetable !== []) {%>
								<% let i = 1; listTimetable.forEach((element) => { %>
									<tr>
										<td><%= i %></td>
										<td><%= element.classid %></td>
										<td><%= element.classname %></td>
										<td><%= element.teacherid %></td>
										<td><%= element.teachername %></td>
										<% let date = new Date(Date.parse(element.date)).toLocaleString('en-US', {
											timeZone: 'Asia/Ho_Chi_Minh'
										}) %>
										<td><%= date %></td>
										<%  let link = "/user/" + teacherId + "/" + element._id; i++%>
										<td><a href=" <%= link %> " class="link-liststudent">Xem thêm</a></td>
									</tr>
								<% }) } %>
							</tbody>
						</table>
					</div>
				</div>
			</div>
